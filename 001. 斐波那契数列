class Solution:
    """
    @param: : an integer
    @return: an ineger f(n)
    """

    def fibonacci(self, n):
        # write your code here
        self.n = n
        if n==1:
            return 0
        if n==2:
            return 1
        if n==3:
            return 1
            
        a = 1;
        b = 0;
        i = n;
        while i > 3:
            t = a
            a = a + b
            b = t
            i = i - 1
        
        return a + b
        
            
